{
  "expo": {
    "name": "talent",
    "slug": "talent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talent.talent",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Esta aplicación necesita acceso al micrófono para grabar audio.",
        "NSCameraUsageDescription": "Acceso a la cámara es necesario para grabar videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.guidoabelleira.talent"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2c2f3ff4-616a-4688-bdee-de46a6371e56"
      }
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow Talent to access your microphone."
        }
      ]
    ]
  }
}
